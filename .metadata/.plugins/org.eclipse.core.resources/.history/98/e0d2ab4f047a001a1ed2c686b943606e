package PackageLibrary;

import java.util.HashMap;
import java.util.Map;
import java.util.Stack;

public class libraryPilha {
	
	
	public static final Map<String, Integer>  NoTerminals = new HashMap<String, Integer>();
	
	public static void nonTerminalClass(/*String sentenca*/) {
		String sentenca = "S -> aA | ab";
		/*String[] sentencaWithoutNonTerminal; 
		
		sentencaWithoutNonTerminal = sentenca.split(">");
		


		sentenca.split("|");*/

		
		
		 String[] tokens = sentenca.trim().split(">");
	        
	        Stack<String> pilhaTokens = new Stack<>();
	        
	        for (int i = tokens.length-1 ; i>=0; i--){
	            String t = tokens[i];
	            if (!t.contentEquals("")) {  
	                pilhaTokens.push(t);
	                System.out.println(pilhaTokens);
	            }
	        }

		
		
		
		/*if (sentenca.toUpperCase() != null) {
			
		}*/
		
		
		
		
		
	}
	
	public static void main(String[] args) {
		nonTerminalClass();
	}
